{
  "name": "express-typescript-eslint-prettier-template",
  "version": "1.0.0",
  "description": "A template for an expressJS, typescript, eslint & prettier project",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "start": "node ./dist/index.js",
    "dev": "nodemon ./src/index.ts",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prettier:check": "prettier ./src --check --ignore-unknown",
    "prettier:write": "prettier ./src --write --ignore-unknown",
    "prepare": "husky install"
  },
  "keywords": [
    "node",
    "express",
    "typescript",
    "eslint",
    "prettier"
  ],
  "author": "Jake Bisson",
  "license": "MIT",
  "dependencies": {
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
